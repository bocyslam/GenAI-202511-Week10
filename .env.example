# Database Configuration
DB_USER=n8n_user
DB_PASSWORD=changeme_secure_password
DB_NAME=n8n_db

# n8n Configuration
N8N_HOST=localhost
N8N_PORT=3333
N8N_PROTOCOL=http
N8N_SECURE_COOKIE=false

# n8n Webhook Configuration
# This URL is used for webhook callbacks from external services
N8N_WEBHOOK_URL=http://localhost:3333/

# n8n Logging
N8N_LOG_LEVEL=info

# Optional: n8n License Key (if using enterprise)
# N8N_LICENSE_KEY=your_license_key_here

# Optional: Authentication Token for API access
# N8N_AUTH_TOKEN=your_auth_token_here

# Optional: Encryption key for sensitive data
# ENCRYPTION_KEY=your_encryption_key_here
